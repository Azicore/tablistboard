{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
	"version": "1.5.1",
	"author": "Azicore",
	"default_locale": "en",
	"description": "__MSG_extensionDescription__",
	"homepage_url": "https://azisava.sakura.ne.jp/tablistboard/",
	"applications": {
		"gecko": {
			"id": "tablistboard@azisava.sakura.ne.jp",
			"strict_min_version": "60.0"
		}
	},
	"icons": {
		"48": "img/icon48.png",
		"96": "img/icon96.png"
	},
	"permissions": [
		"tabs",
		"tabHide",
		"sessions",
		"storage",
		"history"
	],
	"commands": {
		"start": {
			"description": "__MSG_shortcutStart__ (F1)"
		},
		"_execute_sidebar_action": {
			"description": "__MSG_shortcutSidebar__ (Ctrl+F1)"
		}
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_title": "__MSG_browserActionTitle__",
		"default_icon": {
			"16": "img/icon16.png",
			"32": "img/icon32.png",
			"64": "img/icon64.png"
		}
	},
	"sidebar_action": {
		"browser_style": false,
		"default_panel": "sidebar.html",
		"default_icon": {
			"16": "img/icon16.png",
			"32": "img/icon32.png",
			"64": "img/icon64.png"
		}
	}
}